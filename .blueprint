models:
    User: { full_name: 'string:255 nullable', password: 'string:255 nullable', email: 'unique string:255 nullable', phone: 'unique string:255 nullable', telegram_chat_id: 'string:255 nullable', birthday: 'string:255 nullable', auth_code: 'string:255 nullable', role_id: 'id foreign:roles', email_verified_at: 'string:255 nullable', phone_verified_at: 'string:255 nullable', blocked_at: 'string:255 nullable', relationships: { belongsTo: Role, hasMany: 'Order, Favorite, Review, Location' } }
    Role: { title: 'string:255 nullable', slug: 'unique string:255', relationships: { belongsTo: User } }
    SmsQueue: { phone: 'string:255', message: 'string:255', status: 'integer default:0' }
    Location: { region: "string:255 comment:'Регион'", city: "string:255 comment:'Город'", district: "string:255 nullable comment:'Район'", address: "string:255 comment:'Адрес'", landmark: "string:255 nullable comment:'Ориентир'", latitude: "string:50 nullable comment:'Широта'", longitude: "string:50 nullable comment:'Долгота'", object_type: "integer default:0 comment:'компания \\ пользователь'", object_id: 'id foreign:companies', deleted_at: 'nullable timestamp', relationships: { belongsTo: 'Company, User' } }
    Company: { title: 'string:400', domain: 'string:255 unique', logo: 'string:400 nullable', vk_group: "string:255 nullable comment:'групп вк для обновления товаров'", telegram_channel: "string:255 nullable comment:'канал для заказов'", description: 'longtext nullable', contacts: "json nullable comment:'Массив объектов контаков заведения'", socials: "json nullable comment:'Массив объектов соц. сети'", bots: "json nullable comment:'Массив объектов ботов заведения'", banners: "json nullable comment:'Баннеры заведения'", site_url: "string:255 nullable comment:'Сайт заведения'", is_active: 'boolean default:true', payment_card: "string:255 nullable comment:'Карта владельца компании для автоматизации переводов'", work_time: "json nullable comment:'Время работы заведения'", amo_link: "string:255 nullable comment:'ссылка подключения к АМО CRM'", amo_login: "string:255 nullable comment:'логин от АМО CRM'", amo_password: "string:255 nullable comment:'пароль от АМО CRM'", deleted_at: 'nullable timestamp', relationships: { hasMany: 'Product, ProductCategory', belongsToMany: 'Section, Seo, Service, Story, Order' } }
    Story: { image: 'string:255 nullable', title: 'string:255 nullable', subtitle: 'string:255 nullable', company_id: 'id foreign:companies', author_title: 'string:255 nullable', lifetime: 'integer default:0', publish_at: 'nullable timestamp', deleted_at: 'nullable timestamp', relationships: { belongsTo: Company } }
    Seo: { company_id: 'id foreign:companies', key: 'string:255 nullable', text: 'longtext nullable', relationships: { belongsTo: Company } }
    Section: { title: 'string:255 nullable', description: 'longtext nullable', image: 'string:255 nullable', is_active: 'boolean default:true', relationships: { belongsToMany: Company } }
    Service: { title: 'string:255 nullable', icon: 'string:255 nullable', description: 'longtext nullable', relationships: { belongsToMany: Company } }
    IngredientCategory: { title: 'string:255 nullable', image: 'string:255 nullable', relationships: { hasMany: Ingredient } }
    Ingredient: { title: 'string:255 nullable', description: 'longtext nullable', image: 'string:255 nullable', weight: 'string:255 nullable', price: 'string:255 nullable', ingredient_category_id: 'id foreign:ingredient_categories', product_id: 'id foreign:products', is_checked: 'boolean default:false', is_disabled: 'boolean default:false', relationships: { belongsTo: 'Product, IngredientCategory' } }
    ProductCategory: { title: 'string:255', company_id: 'id foreign:companies', relationships: { belongsTo: Company, belongsToMany: Product } }
    ProductOption: { key: 'string:255 nullable', title: 'string:255 nullable', value: 'string:255 nullable', section: 'string:255 nullable', product_id: 'id foreign:products', relationships: { belongsTo: Product } }
    Product: { title: 'string:255 nullable', description: 'longtext nullable', images: 'json nullable', type: 'integer default:0', status: 'integer default:0', price: 'double default:0', discount_price: 'double default:0', in_stop_list: 'boolean default:false', company_id: 'id foreign:companies', relationships: { belongsTo: Company, belongsToMany: ProductCategory } }
    Favorite: { product_id: 'id foreign:products', user_id: 'id foreign:users', relationships: { belongsTo: 'Product, User' } }
    Review: { text: 'longtext nullable', images: 'json nullable', rating: 'integer default:0', type: 'integer default:0', user_id: 'id foreign:users', object_id: 'id foreign:products', deleted_at: 'nullable timestamp', relationships: { belongsTo: 'Product, Company, User' } }
    Order: { company_id: 'id foreign:companies', user_id: 'id foreign:users', delivery_service_info: "json nullable comment:'Информация о сервисе доставки'", deliveryman_info: "json nullable comment:'Берется из внешнего доверенного сервиса'", product_details: "json nullable comment:'Дамп заказанных продуктов'", product_count: 'integer default:0', summary_price: 'double default:0', delivery_price: 'double default:0', delivery_range: 'double default:0', deliveryman_latitude: 'double default:0', deliveryman_longitude: 'double default:0', delivery_note: 'longtext nullable', receiver_name: 'string:255 nullable', receiver_phone: 'string:255 nullable', receiver_location_id: 'id foreign:locations', status: 'integer default:0', order_type: 'integer default:0', payed_at: 'timestamp nullable', relationships: { belongsTo: 'User, Location, Company' } }
    PaymentHistory: { amount: 'double default:0', user_id: 'id foreign:users', order_id: 'id foreign:orders' }
    MoneyTransfer: { amount: 'double default:0', card: 'string:255', description: 'string:255', status: 'integer default:0' }
